{
    "name": "mostafaznv/nova-video",
    "description": "Video Field for Laravel Nova",
    "keywords": ["laravel", "nova", "video", "video field", "ffmpeg", "stream", "hls", "larupload", "upload video"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "mostafaznv/larupload": "^1.0.0",
        "laravel/nova": "^4.0",
        "laravel/framework": "^10.4.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\NovaVideo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaznv\\NovaVideo\\VideoFieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
